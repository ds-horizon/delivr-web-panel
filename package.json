{
  "name": "codepush-dashboard",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "NODE_OPTIONS=\"--import tsx/esm\" remix vite:build",
    "start": "NODE_ENV=production node server.mjs",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "gen:routes": "yarn routes-gen -d @routes-gen/remix",
    "gen:config": "yarn tsx ./scripts/make-version-file.ts",
    "postinstall": "pnpm gen:routes && pnpm gen:config",
    "test": "NODE_ENV=test pnpm vitest run",
    "test:coverage": "NODE_ENV=test pnpm vitest run --coverage"
  },
  "dependencies": {
    "@mantine/core": "^7.13.2",
    "@mantine/form": "^7.13.4",
    "@mantine/hooks": "^7.13.2",
    "@mantine/notifications": "^7.14.0",
    "@mantine/spotlight": "^7.13.3",
    "@remix-run/express": "^2.14.0",
    "@remix-run/node": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@remix-run/serve": "^2.13.1",
    "@routes-gen/remix": "^0.3.7",
    "@tabler/icons-react": "^3.19.0",
    "add": "^2.0.6",
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "clsx": "^2.1.1",
    "express": "^4.21.1",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "remix-auth": "^3.7.0",
    "remix-auth-google": "^2.0.0",
    "routes-gen": "^0.6.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.13.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "2.1.5",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.2.0",
    "happy-dom": "^15.11.6",
    "postcss": "^8.4.47",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.19.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^2.1.5"
  },
  "engines": {
    "node": ">=18.18.0 <18.18.1"
  }
}
